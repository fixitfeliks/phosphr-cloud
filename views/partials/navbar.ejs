<div style="text-align:center;font-size:20px;font-family:calibri;padding-top:10px;">
  <img style="width: 100%;height:200px;object-fit:contain" src="/images/omw.png">
  <h4><b>Welcome to Fix it Feliks</b><h4>
  <div class="w3-card-4" style="width:68%;height:100%;margin:auto;">
    <% for (var i = 0; i < tempBar.items.length; i++) { %>
      <form action="<%= tempBar.items[i].route %>" method="get">
        <input type="submit" class="w3-btn w3-black w3-ripple" style="width:100%;" value="<%= tempBar.items[i].name %>" /> <br>
      </form>
    <% } %>
  </div>
</div>
